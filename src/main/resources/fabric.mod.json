{
  "schemaVersion": 1,
  "id": "meteor-extended",
  "version": "${version}",
  "name": "Meteor Extended",
  "description": "More modules for the meteor client",
  "authors": [
    "imtouchk"
  ],
  "contact": {
    "repo": "https://github.com/imtouchk/meteor-extended"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "net.imtouchk.meteorextended.MeteorExtendedAddon"
    ]
  },
  "mixins": [
    "addon-template.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=1.19.4",
    "meteor-client": "*",
    "fabric-language-kotlin": ">=1.9.6+kotlin.1.8.22"
  }
}
